config wifi-device 'radio0'
	option phy 'phy0'
	option type 'mac80211'
	option band '2.4GHz'
	option channel 'auto'
	option hwmode '11bgnax'
	option htmode 'VHT20'
	option disabled '0'
	option country 'US'
	option acs_fallback_chan '1 5 40'
	option beacon_int '100'
	option short_gi_20 '1'
	option short_gi_40 '1'
	option txpower '100'
	option acs_scan_mode '1'
	option obss_interval '300'
	option ignore_40_mhz_intolerant '0'

config wifi-iface 'default_radio24G'
	option device 'radio0'
	option ifname 'wlan0'
	option macaddr '00:50:F1:20:E2:93'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option encryption 'psk2'
	option key 'test_passphrase'
	option ssid 'dummy_ssid_2.4G'
	option max_inactivity '60'
	option wps_pushbutton '0'

config wifi-iface 'default_radio1'
	option ssid 'test_ssid'
	option device 'radio0'
	option ifname 'wlan0.1'
	option macaddr '00:50:F1:20:E2:94'
	option mode 'ap'
	option network 'lan'
	option disabled '0'
	option hidden '0'
	option wmm '1'
	option uapsd '1'
	option isolate '0'
	option encryption 'psk2'
	option key1 '123456789a'
	option key 'test_passphrase'
	option wpa_group_rekey '3600'
	option ieee80211w '0'
	option macfilter 'Disabled'
	option wps_pushbutton '1'

config wifi-iface 'default_ep1'
	option device 'radio0'
	option ifname 'wlan1'
	option macaddr '00:50:F1:20:E2:95'
	option network 'lan'
	option mode 'sta'
	option disabled '1'

