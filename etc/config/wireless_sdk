config wifi-device 'radio4'
	option phy 'phy2'
	option type 'mac80211'
	option band '6GHz'
	option channel '1'
	option hwmode '11anacax'
	option htmode 'VHT160'
	option disabled '0'
	option country 'US'
	option acs_fallback_chan '36 40 40'
	option beacon_int '100'
	option doth '1'
	option short_gi_20 '1'
	option short_gi_40 '1'
	option txpower '100'
    
config wifi-iface 'default_radio6G'
	option device 'radio4'
	option ifname 'wlan4'
	option macaddr '00:50:F1:20:E2:A4'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option encryption 'psk2'
	option key 'test_passphrase'
	option ssid 'dummy_ssid_6G'
	option max_inactivity '60'
	option wps_pushbutton '0'
    
config wifi-device 'radio2'
	option phy 'phy1'
	option type 'mac80211'
	option band '5GHz'
	option channel 'auto'
	option hwmode '11anacax'
	option htmode 'VHT160'
	option disabled '0'
	option country 'US'
	option acs_fallback_chan '36 40 40'
	option beacon_int '100'
	option doth '1'
	option short_gi_20 '1'
	option short_gi_40 '1'
	option txpower '100'
	option acs_scan_mode '1'

