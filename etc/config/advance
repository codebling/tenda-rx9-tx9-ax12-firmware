config interface 'remweb'
    option enable '0'
    option ip '0.0.0.0'
    option port '8888'

config interface 'ddns'
    option enable '0'
    option srv_name ''
    option username ''
    option password ''
    option domain ''
    option status '0'

config interface 'secure'
    option login_pwd '1'
    option wlan_pwd '1'
    option dns_hijack '1'
    option ddos_atk '1'

config firewall 'safety'
    option icmp '1'
    option tcp '1'
    option udp '0'
    option sync '0'
    option fast '1'
    option CE_mode '1'
